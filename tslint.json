{
    "extends": "tslint:recommended",
    "rules": {
        "no-console": {
            "options": [
                "debug",
                "info",
                "log",
                "table",
                "time",
                "timeEnd",
                "trace"
            ]
        },
        "array-type": [true, "generic"],
        "ordered-imports": true,
        "no-null-keyword": true,
        "prefer-const": true,
        "jsdoc-format": true
    }
}